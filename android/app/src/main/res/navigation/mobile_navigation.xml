<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.xgd.axxlab.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.xgd.axxlab.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
    android:id="@+id/navigation_notifications"
    android:name="com.xgd.axxlab.ui.notifications.NotificationsFragment"
    android:label="@string/title_notifications"
    tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.xgd.axxlab.ui.settings.SettingsFragment"
        android:label="@string/title_thanks"
        tools:layout="@layout/fragment_thanks" />
    <fragment
        android:id="@+id/navigation_thanks"
        android:name="com.xgd.axxlab.ui.thanks.ThanksFragment"
        android:label="@string/title_thanks"
        tools:layout="@layout/fragment_thanks" />
</navigation>